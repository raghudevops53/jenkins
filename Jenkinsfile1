pipeline {
  agent any

  stages {

    stage('One') {
      steps {
        sh 'echo Welcome to Stage One'
      }
    }

    stage('Two') {
      steps {
        sh 'echo Welcome to Stage Two'
      }
    }

    stage('Threee') {
      steps  {
        sh 'echo Welcome to Stage Three'
      }
    }

  } // stages

} // pipeline