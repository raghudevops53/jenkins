pipeline {
  agent any

  environment {
    URL = "google.com"
  }

  stages {

    stage('One') {
      steps {
        sh 'echo URL=${URL}'
      }
    }

    stage('Two') {
      steps {
        sh 'echo Welcome to Stage Two'
      }
    }

    stage('Threee') {
      steps  {
        sh 'echo Welcome to Stage Three'
      }
    }

  } // stages

} // pipeline