pipeline {
  agent any

  environment {
    URL1 = "google.com"
    USERPASS = credentials('RAGHU')
  }

  stages {

    stage('One') {
      steps {
        sh 'echo URL=${URL1}'
      }
    }

    stage('Two') {
      steps {
        sh 'echo Welcome to Stage Two'
      }
    }

    stage('Threee') {
      environment {
        URL1 = "yahoo.com"
      }
      steps  {
        sh 'echo URL = ${URL1}'
        sh 'echo USERPASS = ${USERPASS}'
      }
    }

  } // stages

} // pipeline